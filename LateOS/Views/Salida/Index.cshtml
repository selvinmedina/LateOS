@model IEnumerable<LateOS.Models.tbSalida>

@{
    ViewBag.Title = "Listado de Salidas";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="col-lg-12">
<div class="ibox float-e-margins">
<div class="ibox-title">

	<h2>Listado de Salidas</h2>
</div>
<div class="ibox-content">
<table class="table table-striped table-bordered dataTable">
<thead>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.sali_Existencia)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.sali_Cantidad)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.sali_FechaEntrada)
        </th>
        <th>
           Código Producto
        </th>

    </tr>
</thead>
<tbody>
@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.sali_Existencia)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.sali_Cantidad)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.sali_FechaEntrada)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tbProducto.prod_Codigo)
        </td>
        
    </tr>
}
</tbody>
</table>
</div>
</div>
</div>
