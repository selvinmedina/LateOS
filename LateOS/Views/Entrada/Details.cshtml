@model LateOS.Models.tbEntrada

@{
    ViewBag.Title = "Detalle de Entradas";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="col-lg-12">
    <div class="ibox float-e-margins">
        <div class="ibox-title">
            @Html.ActionLink("Regresar", "Index", null, new { @class = "pull-right" })
            <h2>Detalle de Entradas</h2>
        </div>

        <div class="ibox-content">
            <dl class="dl-horizontal">
                <dt>
                    @Html.DisplayNameFor(model => model.ent_Existencia)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.ent_Existencia)
                </dd>

                <dt>
                    @Html.DisplayNameFor(model => model.ent_Cantidad)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.ent_Cantidad)
                </dd>

                <dt>
                    @Html.DisplayNameFor(model => model.ent_FechaEntrada)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.ent_FechaEntrada)
                </dd>

                <dt>
                    Código Producto
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.tbProducto.prod_Codigo)
                </dd>

                <dt>
                    Producto
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.tbProducto.prod_Descripcion)
                </dd>

            </dl>
            <p>
                
                @Html.ActionLink("Cancelar", "Index", null, new { @class = "btn btn-danger" })
            </p>
        </div>
    </div>
</div>
<div class="ibox float-e-margins">
    <div class="ibox-title">
        <h2>Auditoría</h2>
    </div>
    <div class="ibox-content">
        <div class="form-horizontal">
            <table class="table table-striped table-bordered dataTable">
                <thead>
                    <tr>
                        <th>Acción</th>
                        <th>Usuario</th>
                        <th>Fecha</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>Creado</td>
                        <td>@Html.DisplayFor(model => model.tbUsuarios.usu_NombreUsuario)</td>
                        <td>@Html.DisplayFor(model => model.ent_FechaCrea)</td>
                    </tr>
                    <tr>
                        <td>Modificado</td>
                        <td>@Html.DisplayFor(model => model.tbUsuarios1.usu_NombreUsuario)</td>
                        <td>@Html.DisplayFor(model => model.ent_FechaModifica)</td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>
