@model IEnumerable<LateOS.Models.tbEntrada>

@{
    ViewBag.Title = "Listado de Entradas";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="col-lg-12">
<div class="ibox float-e-margins">
<div class="ibox-title">
	@Html.ActionLink("Registrar ", "Create", null, new {@class="btn btn-primary pull-right" })
	<h2>Listado de Entradas</h2>
</div>
<div class="ibox-content">
<table class="table table-striped table-bordered dataTable">
<thead>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ent_Existencia)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ent_Cantidad)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ent_FechaEntrada)
        </th>
        <th>
           Código Producto
        </th>
        <th>
         Producto
        </th>
        <th></th>
    </tr>
</thead>
<tbody>
@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ent_Existencia)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ent_Cantidad)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ent_FechaEntrada)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tbProducto.prod_Codigo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tbProducto.prod_Descripcion)
        </td>
        <td>
            @Html.ActionLink("Detalles", "Details", new { id=item.ent_Id }, new {@class="btn btn-info btn-xs"})
        </td>
    </tr>
}
</tbody>
</table>
</div>
</div>
</div>
