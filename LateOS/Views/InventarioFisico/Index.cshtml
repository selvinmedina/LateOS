@model IEnumerable<LateOS.Models.InventarioFisico>

@{
    ViewBag.Title = "Listado Inventario";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    @*Botton de regresar aqui*@
                    <h2>Listado Inventario</h2>
                    
                </div>
                <div class="ibox-content">
                    <div class="table-responsive">
                        <div id="DataTables_Table_0_wrapper" class="dataTables_wrapper form-inline dt-bootstrap">
                            @*DataTables_Table_0*@
                            <table class="table table-striped table-bordered table-hover dataTables-example dataTable dtr-inline" id="tblCliente" aria-describedby="DataTables_Table_0_info" role="grid">
                                <thead>
                                    <tr role="row">
                                        <th class="sorting_asc" tabindex="0" aria-controls="DataTables_Table_0" rowspan="1" colspan="1" aria-sort="ascending" aria-label="Rendering engine: activate to sort column descending" style="width: 210px;">@Html.DisplayNameFor(model => model.tbProducto.prod_Codigo)</th>
                                        <th class="sorting" tabindex="0" aria-controls="DataTables_Table_0" rowspan="1" colspan="1" aria-label="Browser: activate to sort column ascending" style="width: 260px;">@Html.DisplayNameFor(model => model.tbProducto.prod_Descripcion)</th>
                                        <th class="sorting" tabindex="0" aria-controls="DataTables_Table_0" rowspan="1" colspan="1" aria-label="Platform(s): activate to sort column ascending" style="width: 235px;">@Html.DisplayNameFor(model => model.invf_total)</th>
                                        <th class="sorting" tabindex="0" aria-controls="DataTables_Table_0" rowspan="1" colspan="1" aria-label="Engine version: activate to sort column ascending" style="width: 180px;">@Html.DisplayNameFor(model => model.invf_FechaInventario)</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model)
                                    {
                                        <tr class="gradeA odd" role="row">
                                            <td>
                                                @Html.DisplayFor(modelItem => item.tbProducto.prod_Codigo)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.tbProducto.prod_Descripcion)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.invf_total)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.invf_FechaInventario)
                                            </td>
                                        </tr>
                                    }
                                </tbody>
                                
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

