
@{
    ViewBag.Title = "InfoStatus";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
<div class="col-lg-12">
    <div class="ibox float-e-margins">
        <div class="ibox-title">
            @Html.ActionLink("Regresar", "Index", null, new { @class = "pull-right" })
            <h2>Información general</h2>
        </div>

        <div class="ibox-content">
            <div class="row">

            </div>
            <div class="row">
                <div class="form-group col-md-4 col-md-offset-1">
                    <div id="piechart" style="width: 500px; height: 500px;"></div>
                </div>
                <div class="form-group col-md-5 col-md-offset-1">
                    <div id="donutchart" style="width: 500px; height: 500px;"></div>
                </div>
            </div>
        </div>
    </div>
</div>

<script type="text/javascript">
    window.onload = function () {
        $.getScript("../scripts/App/General/InfoStatus.js")
          .done(function (script, textStatus) {
              console.log(textStatus);
          })
          .fail(function (jqxhr, settings, exception) {
              console.log("No se pudo recuperar Script InfoStatus");
          });
    };
</script>



