@model IEnumerable<LateOS.Models.tbCiudad>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="col-lg-12">
<div class="ibox float-e-margins">
<div class="ibox-title">
	@Html.ActionLink("Registrar ", "Create", null, new {@class="btn btn-primary pull-right" })
	<h2>Index</h2>
</div>
<div class="ibox-content">
<table class="table table-striped table-bordered dataTable">
<thead>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ciu_Descripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ciu_FechaCrea)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ciu_FechaModifica)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tbUsuarios.usu_NombreUsuario)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tbUsuarios1.usu_NombreUsuario)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tbPais.pais_Descripcion)
        </th>
        <th></th>
    </tr>
</thead>
<tbody>
@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ciu_Descripcion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ciu_FechaCrea)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ciu_FechaModifica)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tbUsuarios.usu_NombreUsuario)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tbUsuarios1.usu_NombreUsuario)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tbPais.pais_Descripcion)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.ciu_Id }, new {@class="btn btn-primary btn-xs"})
            @Html.ActionLink("Detalles", "Details", new { id=item.ciu_Id }, new {@class="btn btn-default btn-xs"})
        </td>
    </tr>
}
</tbody>
</table>
</div>
</div>
</div>
